package com.testing.program;

public final class CustomClass {
	
	private final String name;
	private final Adress address;

	public CustomClass(String name ,Adress address){
		
		this.name=name;
		this.address= new Adress(address.getAdName());
		
	}

	public String getName() {
		return name;
	}

	public Adress getAddress() {
		return address;
	}

	@Override
	public String toString() {
		System.out.println(this.hashCode());
		return "CustomClass [name=" + name + ", address=" + address + "]";
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((address == null) ? 0 : address.hashCode());
		result = prime * result + ((name == null) ? 0 : name.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		CustomClass other = (CustomClass) obj;
		if (address == null) {
			if (other.address != null)
				return false;
		} else if (!address.equals(other.address))
			return false;
		if (name == null) {
			if (other.name != null)
				return false;
		} else if (!name.equals(other.name))
			return false;
		return true;
	}
	
}
