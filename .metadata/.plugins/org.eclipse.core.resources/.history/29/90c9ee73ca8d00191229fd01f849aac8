package com.testing.program;

public class Adress {
	
  private String adName;
  
 public Adress(String name){
	 this.adName=name;
 }

public String getAdName() {
	return adName;
}

public void setAdName(String adName) {
	this.adName = adName;
}

@Override
public String toString() {
	return "Adress [adName=" + adName + "]";
}

}
